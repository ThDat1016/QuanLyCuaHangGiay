package quanlycuahanggiay_java.sanpham;

public class Makhachhang {
	private String MaKH;
	private String loai;
	private int TongChiTieu;
	private String Ho;
	private String Ten;
	private String GioiTinh;
	private String DiaChi;
	private String Email;
	public String getMaKH() {
		return MaKH;
	}
	public void setMaKH(String maKH) {
		MaKH = maKH;
	}
	public String getLoai() {
		return loai;
	}
	public void setLoai(String loai) {
		this.loai = loai;
	}
	public int getTongChiTieu() {
		return TongChiTieu;
	}
	public void setTongChiTieu(int tongChiTieu) {
		TongChiTieu = tongChiTieu;
	}
	public String getHo() {
		return Ho;
	}
	public void setHo(String ho) {
		Ho = ho;
	}
	public String getTen() {
		return Ten;
	}
	public void setTen(String ten) {
		Ten = ten;
	}
	public String getGioiTinh() {
		return GioiTinh;
	}
	public void setGioiTinh(String gioiTinh) {
		GioiTinh = gioiTinh;
	}
	public String getDiaChi() {
		return DiaChi;
	}
	public void setDiaChi(String diaChi) {
		DiaChi = diaChi;
	}
	public String getEmail() {
		return Email;
	}
	public void setEmail(String email) {
		Email = email;
	}
	public Makhachhang(String maKH, String loai, int tongChiTieu, String ho, String ten, String gioiTinh, String diaChi,
			String email) {
		super();
		MaKH = maKH;
		this.loai = loai;
		TongChiTieu = tongChiTieu;
		Ho = ho;
		Ten = ten;
		GioiTinh = gioiTinh;
		DiaChi = diaChi;
		Email = email;
	}
}
